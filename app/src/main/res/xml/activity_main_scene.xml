<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/button4"
            android:layout_width="59dp"
            android:layout_height="55dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="100dp"
            android:layout_marginStart="16dp"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/button5"
            motion:layout_constraintStart_toEndOf="@+id/button4"
            android:layout_width="59dp"
            android:layout_height="55dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="100dp"
            android:layout_marginStart="36dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="0dp"
            android:layout_width="59dp"
            android:id="@+id/button4"
            motion:layout_editor_absoluteX="27dp"
            motion:layout_editor_absoluteY="662dp" />
        <Constraint
            android:layout_height="55dp"
            motion:layout_editor_absoluteX="16dp"
            android:layout_marginBottom="172dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="59dp"
            android:id="@+id/button5" />
        <Constraint
            android:layout_height="55dp"
            motion:layout_editor_absoluteX="311dp"
            android:layout_width="59dp"
            android:id="@+id/button7"
            motion:layout_editor_absoluteY="504dp" />
        <Constraint
            android:layout_height="55dp"
            android:layout_marginBottom="172dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="59dp"
            motion:layout_constraintStart_toEndOf="@+id/button5"
            android:layout_marginLeft="136dp"
            android:layout_marginStart="136dp"
            android:id="@+id/button6" />
    </ConstraintSet>
</MotionScene>